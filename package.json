    "jasmine-core": "*",
    "jasmine-spec-reporter": "*",
    "json-loader": "*",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-coverage": "*",
    "karma-jasmine": "*",
    "karma-mocha-reporter": "*",
    "karma-remap-istanbul": "*",
    "karma-sourcemap-loader": "*",
    "karma-webpack": "*",
    "node-sass": "*",
    "null-loader": "*",
    "postcss-loader": "*",
    "protractor": "*",
